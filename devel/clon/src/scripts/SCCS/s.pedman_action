    catch {exec rm -f ${clonparms}/pedman/Tfiles/cc.tped } result
exec ln -s ${clonparms}/pedman/Tfiles/cc.trans_clasprod_$runum.txt ${clonparms}/pedman/Tfiles/cc.tped
