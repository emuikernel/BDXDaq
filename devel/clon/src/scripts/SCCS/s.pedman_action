}
} elseif { $hraman == "write" } {
if { $ec1_crate == 1 } {
D 6
exec cp ${clonparms}/pedman/archive/ec1.ped ec1.ped
E 6
I 6
#exec cp ${clonparms}/pedman/archive/ec1.ped ec1.ped
E 6
exec pedman_init ec1
exec pedman_write ec1
}
