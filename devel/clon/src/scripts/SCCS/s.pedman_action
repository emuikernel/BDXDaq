E 6
I 6
D 7
#exec cp ${clonparms}/pedman/archive/cc.ped cc.ped
E 6
exec pedman_init cc
exec pedman_write cc
E 7
I 7
#exec cp ${clonparms}/pedman/archive/cc1.ped cc1.ped
exec pedman_init cc1
exec pedman_write cc1
E 7
}
if { $sc_crate == 1 } {
D 6
exec cp ${clonparms}/pedman/archive/sc.ped sc.ped
E 6
