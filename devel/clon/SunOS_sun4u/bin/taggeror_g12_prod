#!/bin/expect -- 
spawn tsconnect "taggeror" 
expect "help." 


send_tty "\n\n\nSetting up g12 production configuration\n\n"




##### MOR1 T1-19

send_tty "\n\n\nSetting up MOR1: T1-19: 0000:0000:0007:FFFF\n\n"

send "19=0000 \r"
expect "OK\r\n"
sleep 1
send "19 \r" 
expect "\r\n" 
sleep 1

send "1A=0000 \r"
expect "OK\r\n"
sleep 1
send "1A \r" 
expect "\r\n" 
sleep 1

send "1B=0007 \r"
expect "OK\r\n"
sleep 1
send "1B \r" 
expect "\r\n" 
sleep 1


send "1C=FFFF \r"
expect "OK\r\n"
sleep 1
send "1C \r" 
expect "\r\n" 
sleep 1

##################### end MOR1
##### MOR2 T20-47
send_tty "\n\n\nSetting up MOR2: T20-47: 0000:7FFF:FFF8:0000\n\n"

send "1D=0000 \r"
expect "OK\r\n"
sleep 1
send "1D \r" 
expect "\r\n" 
sleep 1

send "1E=7FFF \r"
expect "OK\r\n"
sleep 1
send "1E \r" 
expect "\r\n" 
sleep 1

send "1F=FFF8 \r"
expect "OK\r\n"
sleep 1
send "1F \r" 
expect "\r\n" 
sleep 1

send "20=0000 \r"
expect "OK\r\n"
sleep 1
send "20 \r" 
expect "\r\n" 
sleep 1

################################## end MOR2
### MOR3 T1-61 for TAC runs
send_tty "\n\n\nSetting up MOR3: T1-61: FFFF:FFFF:FFFF:FFFF\n\n"

send "21=FFFF \r"
expect "OK\r\n"
sleep 1
send "21 \r" 
expect "\r\n" 
sleep 1

send "22=FFFF \r"
expect "OK\r\n"
sleep 1
send "22 \r" 
expect "\r\n" 
sleep 1

send "23=FFFF \r"
expect "OK\r\n"
sleep 1
send "23 \r" 
expect "\r\n" 
sleep 1

send "24=FFFF \r"
expect "OK\r\n"
sleep 1
send "24 \r" 
expect "\r\n" 
sleep 1

################################ end MOR3
### MOR4 same as MOR1 for scaler
send_tty "\n\n\nSetting up MOR4: T1-19: 0000:0000:0007:FFFF\n\n"

send "25=0000 \r"
expect "OK\r\n"
sleep 1
send "25 \r" 
expect "\r\n" 
sleep 1

send "26=0000 \r"
expect "OK\r\n"
sleep 1
send "26 \r" 
expect "\r\n" 
sleep 1

send "27=0007 \r"
expect "OK\r\n"
sleep 1
send "27 \r" 
expect "\r\n" 
sleep 1

send "28=FFFF \r"
expect "OK\r\n"
sleep 1
send "28 \r" 
expect "\r\n" 
sleep 1

########################### end MOR4
send_tty "\n\n\nIf there are no ERRORs \\nand readback is the same as the input we are done.\n\n"

send "~q" 
exit 
