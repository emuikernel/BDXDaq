{
	field(DISV,"0")
	field(SDIS,"frwd_scaler_mode.VAL NPP NMS")
	field(OMSL,"closed_loop")
	field(DOL,"frwd_scaler_mode.VAL NPP NMS")
	field(OUT,"frwd_scaler.CNT PP MS")
	field(ZNAM,"DONE")
	field(ONAM,"COUNT")
}

