record(ai,"AI:CARD$(card):SIG$(signal)") {
     field(SCAN,"1 second")
  field(DTYP,"V450")
  field(INP,"#C$(card) S$(signal) @")
#     field(FLNK,"AI:CARD$(card):SIG$(signal):counter")
  field(PREC,"4")
  field(LINR,"LINEAR")
  field(EGUF,"10")
  field(EGUL,"-10")
  field(EGU,"Volt")
  field(HOPR,"10")
  field(LOPR,"-10")

}

