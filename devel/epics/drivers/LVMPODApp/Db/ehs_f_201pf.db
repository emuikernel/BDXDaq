record(stringin, "$(PORT):$(CH):name")
{
  field(DESC, "Channel Name")
  field(DTYP, "Snmp")
  field(SCAN, "$(SCAN)")
  field(INP,  "@$(HOST) $(COMMUNITY) WIENER-CRATE-MIB::outputName.u$(CH) STRING: 100")
}


