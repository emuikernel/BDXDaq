setenv LD_LIBRARY_PATH /misc/clas/clas12/R3.14.12.3/QtDM/AAA/extensions/lib/linux-x86:"$LD_LIBRARY_PATH"
setenv QT_PLUGIN_PATH /misc/clas/clas12/R3.14.12.3/QtDM/AAA/extensions/lib/linux-x86:"$QT_PLUGIN_PATH"

