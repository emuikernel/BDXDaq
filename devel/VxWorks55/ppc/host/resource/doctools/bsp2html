IN  Ref				# process doc-only files
OUT Html			# output HTML

ROUTINES no			# Default to no subroutines

# Library index file, and how to refer to it

LIBIX  "libIndex.htm {Board Support Packages}"

# Routine index file, and how to refer to it

ROUTIX "{} {}"
