# setting the TCL_LIBRARY environment variable \
TCL_LIBRARY=$WIND_BASE/host/tcl/tcl; export TCL_LIBRARY

# now invoking wtxtcl, and exit this bourne shell \
wtxtcl $WIND_BASE/host/resource/tcl/app-config/Project/makegen.tcl "$@"
