#  initializations for all rocs
cd "$CODA/VXWORKS_ppc/bootscripts"
< all.boot

#  standard CLAS libraries
cd "$CODA/VXWORKS_ppc/obj"
ld < all_rocs.o

#set env variables
cd "$CODA/VXWORKS_ppc/bootscripts"
< vxbootenv.boot




