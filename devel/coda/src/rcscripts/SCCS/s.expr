foreach i (`ls`)
    if ($i == "SCCS") then
	echo "...skipping SCCS"

    else if (-d $i) then
	echo "...copying directory $i"
	rm -Rf $CLON_RCSCRIPTS/$i
	mkdir  $CLON_RCSCRIPTS/$i
	cd $i;
	foreach j (`ls`)
	    if($j != SCCS) then
		cp $j $CLON_RCSCRIPTS/$i
	    endif
	end
	cd ..
	chmod -R ug+rwx $CLON_RCSCRIPTS/$i
	chmod -R a+x    $CLON_RCSCRIPTS/$i

    else
	echo "...skipping file $i"
    endif
E 4
