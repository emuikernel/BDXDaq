< vxbootenv.boot


#Load coda_roc
cd "$CODA/VXWORKS_ppc/bin"
ld < coda_ts
ld < tcpServer.o
ld < blaster.o
ld < blastee.o


# should we load entire 'librol.so' ???
cd "$CODA/VXWORKS_ppc/obj"
ld < ts.o

taskSpawn "TCP_SERVER",250,0,100000,tcpServer

